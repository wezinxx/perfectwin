{
  "name": "PerfectWin",
  "short_name": "PWin",
  "start_url": "/perfectwin/",
  "display": "standalone",
  "display_override": ["window-controls-overlay"],
  "background_color": "#ffffff",
  "theme_color": "#0d6efd",
  "description": "App de palpites e controle de banca para apostas esportivas.",
  "id": "/",
  "icons": [
    {
      "src": "icon.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "icon512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "launch_handler": {
    "client_mode": "auto"
  },
  "edge_side_panel": {
    "preferred_width": 330
  },
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "application/json": [".json"]
      }
    }
  ],
  "handle_links": "preferred",
  "protocol_handlers": [
    {
      "protocol": "web+perfectwin",
      "url": "/?url=%s"
    }
  ]
}
